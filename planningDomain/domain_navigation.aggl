===

stop : active(1)
{
	{
		1:robot(1,1)
		2:status(2,2)
		1->2(hasStatus)
		1->2(stop)*
	}
	=>
	{
		1:robot(1,1)
		2:status(2,2)
		1->2(hasStatus)
		1->2(stop)
	}
}

changeRoom : active(1)
{
	{
		s2:roomSt(25,75)
		s1:roomSt(-200,75)
		r1:room(-200,-35)
		r2:room(25,-35)
		robot:robot(-95,-145)
		robot->r1(in)
		r1->r2(transitable)
		r1->s1(room)
		r2->s2(room)
	}
	=>
	{
		s2:roomSt(25,75)
		s1:roomSt(-200,75)
		r1:room(-200,-35)
		r2:room(25,-35)
		robot:robot(-95,-145)
		robot->r2(in)
		r1->r2(transitable)
		r1->s1(room)
		r2->s2(room)
	}
	effect
	{
		(forall
			something:object
			(when
				(in something robot)
				(and
					(not (in something s1))
					(in something s2)
				)
			)
		)
	}
}

